  width:900rpx;
  border:lavender 8rpx outset;
  margin:10rpx;  
  box-sizing:border-box;
  box-shadow: 0rpx 6rpx 6rpx 6rpx rgb(197, 197, 206) inset;
  display: flex;
}
.image{
  height: 130rpx;

}
.hoder{
